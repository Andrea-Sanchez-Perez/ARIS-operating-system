package s.o_aris;

/**
 *
 * @author ALUMNO-43
 */
public class SO_ARIS {

    public static void main(String[] args) {
            Inicio pre= new Inicio();
            pre.setVisible(true);
            usuarios cambiar= new usuarios();
            try{
                for (int i=0; i<=100; i++){
                    Thread.sleep(53);
                    pre.porcentaje.setText(Integer.toString(i)+"%");
                    pre.barra.setValue(i);
                    
                    if(i==100){
                        pre.setVisible(false);
                        cambiar.setVisible(true);
                    }
                }
                
            }catch (Exception e){
                
            }
            
    }
    
}
